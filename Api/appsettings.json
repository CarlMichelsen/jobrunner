{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Browser": {
    "WebSocketEndpoint": null
  },
  "GenericJobs": {
    "RoyalRunJob": {
      "Interval": "00:03:00",
      "EnableJitter": true
    }
  }
}
